FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 100000;
    }

    outlet
    {
        type            waveTransmissive;
        field           p;
        psi             thermo:psi;
        gamma           1.4;
        fieldInf        100000;
        lInf            1;
        value           uniform 100000;
    }

    wall
    {
        type            zeroGradient;
    }

    cylinderWall
    {
        type            zeroGradient;
    }
    
    symmetryPlane
    {
        type            symmetry;
    }
    
    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
